## Mode (optional)
# Leave unset to auto-detect based on Firebase envs.
# Set to true/false to force mode.
# EXPO_PUBLIC_USE_MOCK=true

## Whitelabel defaults (optional)
# Brand id: bytebank | heliobank
# EXPO_PUBLIC_BRAND=bytebank
# Theme mode: light | dark
# EXPO_PUBLIC_THEME_MODE=light

# Firebase (required for real mode)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=

# Google Sign-In (Expo AuthSession)
# For local dev with Expo Go, you can often use a single Expo client id
# EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID=...
# For standalone builds / EAS, set platform-specific client IDs below
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=
