# development@v3
EXPO_PUBLIC_USE_MOCK=

## App configs
EXPO_PUBLIC_APP_NAME=
EXPO_PUBLIC_APP_SLUG=
EXPO_PUBLIC_APP_SCHEME=
EXPO_PUBLIC_APP_VERSION=
EXPO_PUBLIC_BUNDLE_IDENTIFIER=
EXPO_PUBLIC_ANDROID_PACKAGE=
EXPO_PUBLIC_PROJECT_ID=
EXPO_GOOGLE_APP_ID=

# Firebase (set when USE_MOCK=false)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_DATABASE_URL=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=

# Google Sign-In Configuration
# IMPORTANT: You need to configure these Client IDs from Google Cloud Console
# 1. Go to https://console.cloud.google.com/
# 2. Select your project
# 3. Go to APIs & Services > Credentials
# 4. Create OAuth 2.0 Client IDs for each platform:

# For Expo Go development (optional - can use platform-specific ones)
EXPO_PUBLIC_GOOGLE_EXPO_CLIENT_ID=

# iOS Client ID (for standalone iOS builds)
EXPO_PUBLIC_GOOGLE_IOS_CLIENT_ID=

# Android Client ID (for standalone Android builds)
EXPO_PUBLIC_GOOGLE_ANDROID_CLIENT_ID=

# Web Client ID (required for Firebase Auth)
EXPO_PUBLIC_GOOGLE_WEB_CLIENT_ID=

# External apis
EXPO_PUBLIC_BRAPI_TOKEN=

# Android
ANDROID_SHA1=
ANDROID-SHA-256=